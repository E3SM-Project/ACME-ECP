RTE_SRC = \
  mo_rte_kind.o \
  mo_optical_props_kernels.o \
  mo_optical_props.o \
	mo_spectral_disc.o \
  mo_fluxes.o \
  mo_rte_solver_kernels.o \
  mo_rte_lw_solver.o \
  mo_rte_lw.o \
  mo_rte_sw_solver.o \
  mo_rte_sw.o

  ##################################
  # RTE - Radiative transfer for energetics
  ##################################
  #
  #
  # Numeric kinds
  #
  mo_rte_kind.o: mo_rte_kind.F90
  #
  # Optical properties
  #
  mo_optical_props_kernels.o: mo_rte_kind.o mo_optical_props_kernels.F90
  mo_optical_props.o:         mo_rte_kind.o mo_optical_props_kernels.o   mo_optical_props.F90
  #
  # Spectral discretization
  #
  mo_spectral_disc.o: mo_rte_kind.o mo_spectral_disc.F90
  #
  # Flux reduction
  #
  mo_fluxes.o:        mo_rte_kind.o mo_spectral_disc.o   mo_fluxes.F90
  #
  # Radiative transfer
  #
  mo_rte_solver_kernels.o: mo_rte_kind.o   \
  	mo_rte_solver_kernels.F90

  mo_rte_lw_solver.o: mo_rte_kind.o \
  	mo_rte_solver_kernels.o \
  	mo_optical_props.o \
  	mo_rte_lw_solver.F90

  mo_rte_sw_solver.o: mo_rte_kind.o \
    mo_rte_solver_kernels.o \
  	mo_optical_props.o \
  	mo_rte_sw_solver.F90

  mo_rte_lw.o: mo_rte_kind.o \
  	mo_optical_props.o  \
  	mo_fluxes.o \
  	mo_rte_lw_solver.o \
  	mo_rte_lw.F90

  mo_rte_sw.o: mo_rte_kind.o \
  	mo_optical_props.o  \
  	mo_fluxes.o \
  	mo_rte_sw_solver.o \
  	mo_rte_sw.F90
